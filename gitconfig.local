[alias]
  graph = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
  bm = branch --merged
  cam = commit --amend
  cm = commit -m
  pp = pull --prune
  pot = push -u origin
  fp = fetch --prune
  ri = rebase -i
  rim = rebase -i master
[user]
  name = Jessie Keck
  email = jessie.keck@gmail.com
[core]
  editor = mate -w
  excludesfile = /Users/jkeck/.gitignore_global
[credential]
  helper = osxkeychain
